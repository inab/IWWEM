<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
	<localRepository>${env.IWWEM_WEBLAUNCHER}/cgi-bin/Backends/t2backend-spring</localRepository>
	<profiles>
		<profile>
			<id>override</id>
			<activation>
				<activeByDefault>false</activeByDefault>
			</activation>
			<repositories>
				<repository>
					<releases />
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<id>mygrid-repository</id>
					<name>myGrid Repository</name>
					<url>http://www.mygrid.org.uk/maven/repository</url>
				</repository>
				<repository>
					<releases />
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<id>mygrid-repo1</id>
					<name>myGrid Repo1</name>
					<url>http://www.mygrid.org.uk/maven/repo1/</url>
				</repository>
				<repository>
					<releases />
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<id>mygrid-biomoby</id>
					<name>myGrid BioMOBY</name>
					<url>http://www.mygrid.org.uk/maven/biomoby/biomoby.org/m2repo</url>
				</repository>
				<repository>
					<releases />
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<id>odf-toolkit</id>
					<name>ODF Toolkit</name>
					<url>http://odftoolkit.org/svn/odfdom~maven2/release/</url>
				</repository>
				<repository>
					<releases />
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<id>tupelo-repository</id>
					<name>tupelo Repository</name>
					<url>http://dlt-dev.ncsa.uiuc.edu/m2/</url>
				</repository>
				<repository>
					<releases>
						<enabled>false</enabled>
					</releases>
					<snapshots />
					<id>mygrid-snapshot-repository</id>
					<name>myGrid Snapshot Repository</name>
					<url>http://www.mygrid.org.uk/maven/snapshot-repository</url>
				</repository>
				<repository>
					<id>mobycentral</id>
					<name>MobyCentral Repository</name>
					<!--
					<url>http://mobycentral.icapture.ubc.ca/maven</url>
					-->
					<url>http://moby.ucalgary.ca/moby_maven/</url>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<releases />
				</repository>
				<!--
				<repository>
					<releases />
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<id>cyclonic</id>
					<name>Cyclonic.org repository</name>
					<url>http://www.cyclonic.org/t2platform/repository</url>
				</repository>
				-->
				<repository>
					<releases />
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<id>restlet</id>
					<name>Restlet repository</name>
					<url>http://maven.restlet.org</url>
				</repository>
				<repository>
					<releases />
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<id>mygrid-mirror</id>
					<name>myGrid maven proxy server</name>
					<url>http://www.mygrid.org.uk/maven/proxy/repository</url>
				</repository>
				<!--
				<repository>
					<releases />
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<id>mygrid-mirror-old</id>
					<name>myGrid maven proxy server</name>
					<url>http://www.mygrid.org.uk/maven/repo1</url>
				</repository>
				-->
			</repositories>
		</profile>
	</profiles>
</settings>
